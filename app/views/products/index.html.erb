
<%= link_to 'New Product', new_product_path, class: "btn btn-default" %>
<h1>Listing products</h1>

<table class="table table-hover">
  <thead>
    <th>#</th>
    <th>Title</th>
    <th>Description</th>
    <th>Image</th>
    <th>Price</th>
  </thead>

  <tbody>
    <% @products.each do |product| %>
    <tr>
      <td><%= product.id %></td>
      <td><%= product.title %></td>
      <td><%= product.description %></td>
      <td><%= product.image_url %></td>
      <td><%= product.price %></td>
     </tr>
  <% end %>
</tbody>
</table>

  




